marks = input("Please enter your marks out of 100: ")
if marks.isnumeric():
  if float(marks)  <= 100:
    marks=float(marks)
    if  marks >=80:
      print("You got an A")
    elif marks >=60:
      print("You got an B")
    elif marks >=50:
      print("You got an C")
    elif marks >=45:
      print("You got an D")
    elif marks >=25:
      print("You got an E")
    elif marks < 25:
      print("You got an F")
  else:
    print("You sure you marks above 100 woahhhhh") 
else:
  print("Invalid input needs to be NUMERICAL")
